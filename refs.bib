@article{saputra_optimasi_2019,
  title        = {Optimasi Fungsi Keanggotaan Fuzzy Mamdani Menggunakan Algoritma Genetika Untuk Penentuan Penerima Beasiswa},
  volume       = {2},
  rights       = {Copyright (c) 2020 {SIMADA} (Jurnal Sistem Informasi dan Manajemen Basis Data)},
